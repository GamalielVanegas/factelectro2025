<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_certificate_import_wizard" model="ir.ui.view">
    <field name="name">certificate.import.wizard.form</field>
    <field name="model">certificate.import.wizard</field>
    <field name="arch" type="xml">
      <form string="Importar Certificado DTE">
        <group>
          <field name="cert_file"/>
          <field name="key_file"/>
          <field name="key_pass"/>
        </group>
        <footer>
          <button string="Importar" type="object" name="action_import" class="btn-primary"/>
          <button string="Cancelar" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>

